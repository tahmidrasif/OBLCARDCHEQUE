@model CardChequeModule.Areas.Teller.Models.TellerDashBoardVM


<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">User Information</h3>
        </div>
        <div class="panel-body">
            <div class="form-group">
                <label class="col-sm-12"><strong>Employee Id</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.EmployeeId, new { @class = "form-control", @readonly = true })</div>
            </div>
            <div class="form-group">
                <label class="col-sm-12"><strong>Name</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.Name, new { @class = "form-control", @readonly = true })</div>
            </div>
            <div class="form-group">
                <label class="col-sm-12"><strong>Email</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.Email, new { @class = "form-control", @readonly = true })</div>
            </div>
            <div class="form-group">
                <label class="col-sm-12"><strong>Job Title</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.JobTitle, new { @class = "form-control", @readonly = true })</div>
            </div>
            <div class="form-group">
                <label class="col-sm-12"><strong>Present Department Name</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.PreDeptName, new { @class = "form-control", @readonly = true })</div>
            </div>
            <div class="form-group">
                <label class="col-sm-12"><strong>Branch Name</strong></label>
                <div class="col-sm-12">@Html.TextBoxFor(x => x.BranchName, new { @class = "form-control", @readonly = true })</div>
            </div>
        </div>
    </div>
</div>